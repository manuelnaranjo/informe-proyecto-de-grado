      <text:p text:style-name="P21">Marcas de usuario</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">M0001 - M2000</text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">Marcas de banderas aritméticas</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">M2003</text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">Marcas de relojes</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">M2009 - M2024</text:p>
     </table:table-cell>
    </table:table-row>
