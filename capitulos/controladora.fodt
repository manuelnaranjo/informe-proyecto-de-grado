     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">Marcas de pantallas</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">M4700 - M4955</text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">Marcas de comunicación con el CNC</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">M5000 – M5957</text:p>
     </table:table-cell>
    </table:table-row>
