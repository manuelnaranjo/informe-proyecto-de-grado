     </table:table-cell>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">M2046 - M2047</text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">Marcas asociadas a los mensajes</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">M4000 - M4127</text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
      <text:p text:style-name="P21">Marcas asociadas a los errores</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabla12.A1" office:value-type="string">
